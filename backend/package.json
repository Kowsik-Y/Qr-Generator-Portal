{
  "name": "quiz-portal-backend",
  "version": "1.0.0",
  "description": "Quiz Portal Backend - Express + PostgreSQL",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "set NODE_ENV=development && nodemon src/server.js",
    "prod": "set NODE_ENV=production && pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:restart": "pm2 restart ecosystem.config.js",
    "prod:delete": "pm2 delete ecosystem.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "quiz",
    "education",
    "backend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "ua-parser-js": "^2.0.6",
    "vm2": "^3.9.19",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  }
}
